#+BEGIN_SRC python :return pltfile :results file 
  from astropy.table import Table
  from matplotlib import pyplot as plt
  tab = Table.read('power-indices-vs-time.tab', format='ascii.tab')

  linedict = {'O': '[O III] 5007', 'H': 'Ha 6563',
              'S': '[S II] 6716', 'N': '[N II] 6583'}
  color_from_view = {'xy': 'magenta', 'xz': 'green', 'yz': 'cyan'}
  symbol_from_ion = {'O': 'o', 'H': 's', 'N': '>', 'S': 'D'}
  markerscale_from_symbol = {'o': 1.0, 's': 0.9, 'D': 0.9, '>': 1.1}
  markerscale_from_view = {'xy': 1.2, 'xz': 1.0, 'yz': 0.8}

  tcut = 20.0

  for ion in 'OHNS':
      k3 = tab['k3_' + ion]
      m = tab['t'] >= tcut
      mm = tab['t'] <= tcut
      for view in ['xy', 'xz', 'yz']:
          kthick = tab['kv' + ion + 'T' + view]
          kthin = tab['kv' + ion + 't' + view]
          plt_kwds = {
              "color": color_from_view[view],
              "marker": symbol_from_ion[ion],
              "linestyle": '-',
              "markersize": 4.0*markerscale_from_view[view] \
                  ,* markerscale_from_symbol[symbol_from_ion[ion]] 
          }
          plt.plot(k3[mm], kthick[mm], alpha=0.2, label=None, **plt_kwds)
          plt.plot(k3[m], kthick[m], alpha=0.6,
                   label=linedict[ion] + ' ' + view,  **plt_kwds)
  kmin, kmax = -4.3, -1.5
  plt.plot([kmin, kmax], [kmin, kmax], '-k')
  plt.plot([kmin, kmax], [kmin + 1, kmax + 1], '--k')
  plt.xlabel(r'Power law index of 3D emissivity, $n$')
  plt.ylabel(r'VCA power law index for thick slice: $\gamma_\mathrm{T}$')
  plt.xlim(kmin, kmax)
  plt.ylim(kmin, kmax)
  plt.title('All ions')
  plt.legend(loc='lower right', fontsize='x-small', ncol=2)
  pltfile = 'vca-thick-vs-3d-all.pdf'
  plt.gcf().set_size_inches((6, 6))
  plt.savefig(pltfile)

#+END_SRC


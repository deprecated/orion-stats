  plt.ylabel(r'Structure function power law index: $m_\mathrm{2D}$')
  plt.xlim(kmin, kmax)
  plt.ylim(kmin+3.5, kmax+3.5)
  plt.legend(loc='upper right', fontsize='x-small', ncol=2)
  pltfile = 'sf-vs-3d-all.pdf'
  plt.gcf().set_size_inches((6, 6))
  plt.savefig(pltfile)


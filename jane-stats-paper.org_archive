  from matplotlib import pyplot as plt
  tab = Table.read('power-indices-vs-time.tab', format='ascii.tab')

  linedict = {'O': '[O III] 5007', 'H': 'Ha 6563',
              'S': '[S II] 6716', 'N': '[N II] 6583'}
  k3 = tab['k3_' + ion]
  for view in ['xy', 'xz', 'yz']:
      kthick = tab['kv' + ion + 'T' + view]
      kthin = tab['kv' + ion + 't' + view]
      plt.plot(k3, kthick, 'o', alpha=0.5)
  kmin, kmax = -4.3, -2.0
  plt.plot([kmin, kmax], [kmin, kmax], '-k')
  plt.xlabel('Power law index of 3D emissivity')
  plt.ylabel('Power law index of thick VCA')
  plt.xlim(kmin, kmax)
  plt.ylim(kmin, kmax)
  plt.title(linedict[ion])
  pltfile = 'vca-thick-vs-3d-{}.pdf'.format(ion)
  plt.gcf().set_size_inches((6, 6))
  plt.savefig(pltfile)

